{
	"name": "Emacs Light",
	"colors": {
		"editor.background": "#f2f2f2",
		"editor.foreground": "#000000"
	},
	"tokenColors": [
        {
            "scope": [
            "comment.line",
            "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#af0000"
            }
        },
        {
            "scope": [
                "comment.block",
                "punctuation.definition.comment.bock"
            ],
            "settings": {
                "foreground": "#87005f"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#934500"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "scope": [
                "keyword",
                "storage"
            ],

            "settings": {
                "foreground": "#a700f6"
            }
        },
        {
            "scope": [
                "keyword.operator",
                "constant.numeric"
            ],

            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#87005f"
            }
        }
	]
}
